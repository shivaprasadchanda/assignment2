Host bastionNSO
  User ubuntu
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no

Host HAproxy
  User ubuntu
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile=~/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devA
  User ubuntu
  ProxyJump bastionNSO
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devB
  User ubuntu
  HostName 91.106.198.84
  ProxyJump bastionNSO
  StrictHostKeyChecking no
  PasswordAuthentication no

Host devC
  User ubuntu
  HostName 91.106.198.49
  ProxyJump bastionNSO
